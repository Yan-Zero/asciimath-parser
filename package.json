{
  "name": "asciimath-parser",
  "version": "0.3.0",
  "type": "module",
  "description": "Transform asciimath to LaTeX",
  "author": "widcardw",
  "license": "MIT",
  "homepage": "https://github.com/widcardw/asciimath-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widcardw/asciimath-parser.git"
  },
  "bugs": "https://github.com/widcardw/asciimath-parser/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "am-parse": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest --run",
    "build": "tsup",
    "release": "nr build && bumpp && npm publish"
  },
  "keywords": [
    "asciimath",
    "LaTeX"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@types/prompts": "^2.4.2",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "eslint": "^8.32.0",
    "prompts": "^2.4.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
